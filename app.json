{
  "expo": {
    "name": "Tanecni sport",
    "slug": "Dance_Sport_App",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.dancesportfamily.tanecnisport",
      "buildNumber": "0.0.4",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 4,
      "package": "com.dancesportfamily.tanecnisport",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "description": "Application for dancers ",
    "githubUrl": "https://github.com/Hajan39/DanceSport"
  }
}